# Hedera Network Configuration
# Get your Account ID and Private Key from: https://portal.hedera.com/dashboard
HEDERA_NETWORK=testnet
HEDERA_ACCOUNT_ID=0.0.YOUR_ACCOUNT_ID
HEDERA_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE

# Multiple Agent Support (for CLI demo)
# Add agent prefixes to this list to manage multiple agents
KNOWN_AGENT_PREFIXES=TODD,DAVE,BOB

# TODD Agent (get credentials from https://portal.hedera.com/dashboard)
TODD_ACCOUNT_ID=0.0.YOUR_TODD_ACCOUNT
TODD_PRIVATE_KEY=YOUR_TODD_PRIVATE_KEY

# DAVE Agent (get credentials from https://portal.hedera.com/dashboard)
DAVE_ACCOUNT_ID=0.0.YOUR_DAVE_ACCOUNT
DAVE_PRIVATE_KEY=YOUR_DAVE_PRIVATE_KEY

# BOB Agent (get credentials from https://portal.hedera.com/dashboard)
BOB_ACCOUNT_ID=0.0.YOUR_BOB_ACCOUNT
BOB_PRIVATE_KEY=YOUR_BOB_PRIVATE_KEY

# AI Configuration (for AI agent demos)
# Get your API key from: https://platform.openai.com/
OPENAI_API_KEY=your_openai_api_key_here

# HGraph API (for data analysis demos)
HGRAPH_API_KEY=your_hgraph_api_key_here

# Registry URL
REGISTRY_URL=https://testnet.hcs.global

# Demo Configuration
DEMO_LOG_LEVEL=info
DEMO_TIMEOUT=30000